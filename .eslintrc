{
  "extends": [
    "@antfu",
    "prettier"
  ],
  "ignorePatterns": ["**/*importmap*", "packages/**"],
  "rules": {
    "no-console": "off",
    "no-cond-assign": "off",
    "antfu/if-newline": "off",
    "no-control-regex": "off",
    "unicorn/escape-case": "off",
    "no-regex-spaces": "off"
  }
}
